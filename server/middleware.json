{
  "initial:before": {
    "loopback#favicon": {}
  },
  "initial": {
    "cors": {
      "params": {
        "origin": true,
        "credentials": false,
        "maxAge": 86400
      }
    },
    "helmet#xssFilter": {},
    "helmet#frameguard": {
      "params": ["deny"]
    },
    "helmet#hsts": {
      "params": {
        "maxAge": 0,
        "includeSubdomains": true
      }
    },
    "helmet#hidePoweredBy": {},
    "helmet#ieNoOpen": {},
    "helmet#noSniff": {},
    "helmet#noCache": {
      "enabled": false
    }
  },
  "session": {},
  "auth": {},
  "log": {},
  "parse": {
    "body-parser#json": {
      "params": {
        "limit": "50mb"
      }
    },
    "body-parser#urlencoded": {
      "params": {
        "extended": true,
        "limit": "50mb"
      }
    }
  },
  "routes": {
    "loopback#rest": {
      "paths": ["${restApiRoot}"]
    }
  },
  "files": {},
  "final": {
    "loopback#urlNotFound": {}
  },
  "final:after": {
    "strong-error-handler": {
      "params": {
        "debug": true,
        "log": true
      }
    }
  }
}
